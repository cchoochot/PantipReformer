{
	"name": "Pantip Reformer",
	"short_name": "Pantip Reformer",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Adjust Pantip user interface.",
	"homepage_url": "https://github.com/cchoochot/PantipReformer",
	"web_accessible_resources": [
		"fonts/*.*"
	],
	//"icons": {
	//	"16": "icons/icon16.png",
	//	"48": "icons/icon48.png",
	//	"128": "icons/icon128.png"
	//},
	"content_scripts": [{
			"run_at": "document_start",
			"matches": [
				"*://pantip.com/*"
			],
			"css": [
				"src/style.css"
			],
			"js": [
				"src/inject.js"
			]
		}
	]
}
